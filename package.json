{
  "name": "barreto-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:wiremock": "ng serve -c wiremock",
    "start:with-mock": "start-server-and-test wiremock http://localhost:8080 start:wiremock",
    "build": "ng build --configuration=production",
    "lint": "ng lint",
    "prettier": "prettier --write \"src/**/*.{ts, html, scss}\"",
    "prepare": "husky install",
    "wiremock": "docker-compose -f ./miscellaneous/wiremock/docker-compose.wiremock.yml up",
    "docker:build": "docker build -t barreto-front .",
    "docker:run": "docker run --name barreto-front-container -d -p 8888:80 barreto-front"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/localize": "~13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@ng-bootstrap/ng-bootstrap": "12.1.2",
    "bootstrap": "5.0.1",
    "bootstrap-icons": "^1.8.3",
    "date-fns": "^2.28.0",
    "ngx-mask": "12.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1400.1",
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular-eslint/builder": "^13.4.0",
    "@angular-eslint/eslint-plugin": "^13.4.0",
    "@angular-eslint/eslint-plugin-template": "^13.4.0",
    "@angular-eslint/template-parser": "^13.4.0",
    "@angular/cli": "~13.3.8",
    "@angular/compiler-cli": "~13.3.11",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "2.6.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.6.4"
  }
}
